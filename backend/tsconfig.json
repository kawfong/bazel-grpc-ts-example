{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable"
    ],
    "allowJs": true,
    "declaration": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "module": "CommonJS",
    "paths": {
      "@/auto_gen/*": ["../proto/*"]
    }
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
